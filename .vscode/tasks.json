{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "CMake: build",
      "type": "shell",
      "command": "cmake --build .",
      "options": {
        "cwd": "${workspaceFolder}/out/build/GCC 14.1.0 x86_64-w64-mingw32 (ucrt64)"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": ["$gcc"]
    }
  ]
}